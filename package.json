{
    "type": "commonjs",
    "name": "mcp-desktop",
    "version": "0.1.0",
    "description": "Microsoft Cloud Platform Desktop Client",
    "main": "src/main/index.cjs",
    "scripts": {
        "dev:web": "node dev-server.cjs",
        "dev:api": "node src/main/server.cjs",
        "start": "electron .",
        "server": "node src/main/combined-server.cjs",
        "dev": "./start-mcp.sh",
        "dev:electron": "nodemon --exec electron . --watch src",
        "dev:debug": "./run-electron-debug.sh",
        "lint": "standard",
        "build": "electron-builder",
        "reset-db": "node scripts/reset-database.js",
        "setup": "npm install && node scripts/reset-database.js"
    },
    "dependencies": {
        "@azure/msal-node": "^2.16.2",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@npmcli/fs": "^3.1.0",
        "axios": "^1.6.2",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.5.0",
        "express-session": "^1.18.1",
        "fs-extra": "^11.2.0",
        "helmet": "^7.2.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "keytar": "^7.9.0",
        "lru-cache": "^10.0.1",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "mysql2": "^3.11.0",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.7.0",
        "node-gyp": "^11.2.0",
        "pg": "^8.11.3",
        "pg-pool": "^3.6.2",
        "sqlite3": "^5.1.6",
        "uuid": "^9.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "concurrently": "^8.2.2",
        "electron": "^28.0.0",
        "electron-builder": "^24.9.1",
        "http-proxy-middleware": "^3.0.5",
        "nodemon": "^3.0.2",
        "serve": "^14.2.0",
        "standard": "^17.1.0"
    },
    "overrides": {
        "glob": "^10.3.10",
        "rimraf": "^5.0.5",
        "boolean": "^3.2.0",
        "npmlog": "^7.0.1",
        "are-we-there-yet": "^4.0.0",
        "gauge": "^5.0.0",
        "@npmcli/move-file": "@npmcli/fs",
        "eslint": "npm:standard@^17.1.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
